# Input do usuário de dois números
num1 = float(input('Digite um número'))
num2 = float(input('Digite outro número'))

# Realizar os calculos matemáticos
soma = num1 + num2
sub = num1 - num2
multi = num1 * num2
div = num1 / num2
expo = num1 ** num2

# Imprimir
print(f'Soma: {soma}')
print(f'Subtração {sub}')
print(f'Multiplicação: {multi}')
print(f'Divisão: {div}')
print(f'Exponenciação: {expo:.2f}')
